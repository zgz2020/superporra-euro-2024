{
  "name": "superporra-euro-2012",
  "version": "1.0.0",
  "description": "A full stack application with React and MongoDB",
  "main": "index.js",
  "scripts": {
    "build": "webpack -p",
    "cy:open": "npx cypress open",
    "cy:test": "cypress run --headless --spec=cypress/tests/*.test.js --browser chrome",
    "deploy": "npm run build && git commit -a -m deployment && git push heroku master",
    "dev": "webpack-dev-server --open",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install npm && run build",
    "initialize": "babel-node src/server/initialize-db",
    "server": "cross-env NODE_ENV=production babel-node src/server",
    "server-dev": "nodemon src/server --exec babel-node src/server",
    "start-dev": "concurrently \"npm run server-dev\" \"npm run dev\"",
    "server-spec": "babel-node src/server/server.spec",
    "start": "npm run server"
  },
  "keywords": [
    "react",
    "redux",
    "express",
    "mongo"
  ],
  "author": "Daniel 'The Code Whisperer' Stern",
  "license": "ISC",
  "dependencies": {
    "auth0-js": "^9.13.4",
    "auth0-lock": "^11.26.3",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-jwt": "^6.0.0",
    "express-jwt-authz": "^2.4.1",
    "history": "^4.7.2",
    "jwks-rsa": "^1.9.0",
    "lodash.clonedeep": "^4.5.0",
    "md5": "^2.2.1",
    "mongodb": "^3.1.8",
    "react": "^16.4.2",
    "react-dom": "^16.5.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "uuid": "^3.3.2",
    "webpack": "^4.17.2"
  },
  "engines": {
    "node": "9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.2",
    "concurrently": "^4.0.1",
    "cross-env": "^5.2.0",
    "cypress": "^4.9.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.4",
    "style-loader": "^0.23.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.7"
  }
}
