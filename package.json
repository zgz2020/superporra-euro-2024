{
  "name": "superporra-euro-2012",
  "version": "1.0.0",
  "description": "A full stack application with React and MongoDB",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production",
    "deploy": "npm run build && git commit -a -m deployment && git push heroku master",
    "dev": "webpack-dev-server --open",
    "initialize": "babel-node src/server/initialize-db",
    "get-emails": "babel-node src/server/getEmails",
    "server": "cross-env NODE_ENV=production babel-node src/server",
    "server-dev": "nodemon src/server --exec babel-node src/server",
    "start-dev": "concurrently \"npm run server-dev\" \"npm run dev\"",
    "server-spec": "babel-node src/server/server.spec",
    "start": "npm run server"
  },
  "keywords": [
    "react",
    "redux",
    "express",
    "mongo"
  ],
  "author": "Daniel 'The Code Whisperer' Stern",
  "license": "ISC",
  "dependencies": {
    "auth0-js": "^9.13.4",
    "auth0-lock": "^12.0.2",
    "axios": "^1.4.0",
    "babel-loader": "^8.0.6",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.3",
    "cors": "^2.8.4",
    "email-validator": "^2.0.4",
    "express": "^4.16.3",
    "express-jwt": "^8.4.1",
    "express-jwt-authz": "^2.4.1",
    "history": "^4.7.2",
    "jwks-rsa": "^3.0.1",
    "lodash.clonedeep": "^4.5.0",
    "md5": "^2.2.1",
    "mongodb": "^3.1.8",
    "nodemailer": "^6.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "uuid": "^3.3.2",
    "webpack": "^5",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": "^20.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "jest": "^25.0.0",
    "nodemon": "^1.14.9",
    "style-loader": "^0.23.0",
    "webpack-dev-server": "^3.1.7"
  }
}
